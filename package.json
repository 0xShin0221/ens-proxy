{
  "name": "ocean-ens-proxy",
  "description": "Ocean Protocol ENS Proxy Server",
  "version": "0.0.0",
  "author": "Ocean Protocol <devops@oceanprotocol.com>",
  "license": "Apache-2.0",
  "main": "./dist/app.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .ts --ext .tsx . && npm run type-check",
    "clean": "rm -rf ./dist",
    "format": "prettier --ignore-path .gitignore './**/*.{css,yml,js,ts,tsx,json}' --write",
    "test:format": "npm run format && npm run lint",
    "type-check": "tsc --noEmit",
    "test:integration": "mocha --config=test/.mocharc.json --node-env=test --exit 'test/**/*.test.ts'",
    "test": "npm run test:format && npm run test:integration"
  },
  "dependencies": {
    "ethers": "^5.7.0",
    "next": "^12.2.5",
    "urql": "^2.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.8.1",
    "typescript": "4.7.3"
  }
}
